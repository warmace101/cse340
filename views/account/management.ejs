<%- include("../partials/header") %>
<main>
  <h1>Account Management</h1>
  <% if (flash && flash.length > 0) { %>
    <div class="flash-message"><%= flash %></div>
  <% } %>
  <% if (errors) { %>
    <ul class="notice">
      <% errors.array().forEach(error => { %>
        <li><%= error.msg %></li>
      <% }) %>
    </ul>
  <% } %>
  <p>You're logged in.</p>
</main>
<%- include("../partials/footer") %>